  "events": [
    {
      "disabled": false,
      "folded": false,
      "type": "BuiltinCommonInstructions::JsCode",
      "inlineCode": "if(runtimeScene.getGame().getInputManager().isMouseButtonReleased(0)){\n    let current = objects.find(item => {\n        return item.getVariableBoolean(item.getVariables().get(\"dragged\"), true);\n    });\n    if (current){\n        objects.forEach(ele => {\n            let mouseX = runtimeScene.getGame().getInputManager().getMouseX();\n            let mouseY = runtimeScene.getGame().getInputManager().getMouseY();\n            if(ele.getName() == \"IF_item_target\" && ele.getX() <= mouseX && mouseX <= ele.getX()+ele.getWidth() && ele.getY() <= mouseY && mouseY <= ele.getY()+ele.getHeight() ){\n                current.setX(ele.getX());\n                current.setY(ele.getY());\n            }\n        });\n    }\n}",
      "parameterObjects": "IF_item_target_group",
      "useStrict": true,
      "eventsSheetExpanded": true
    },
    {
      "disabled": false,
      "folded": false,
      "type": "BuiltinCommonInstructions::JsCode",
      "inlineCode": "\nlet texts = [\n    \"Hast du den 1. Zug?\",\n    \"Gehe in eine Ecke. \\n\\nNächster Zug: Hat der Gegner ein O in \\ndie Ecke gegenüber gesetzt?\",\n    \"Gehe in die Ecke gegenüber von Zug 1.\\n\\nNächster Zug: Ist das Feld zwischen \\nden beiden Eckkreuzen noch frei?\",\n    \"Gehe in eine andere freie Ecke. \\n\\nNächster Zug: Ist das Feld zwischen \\nden beiden Eckkreuzen noch frei?\",\n    \"Gehe in eine freie Ecke. \\n\\nNächster Zug: Hat der Gegner 2 O in \\neiner Reihe?\",\n    \"Blockiere die Reihe. \\n\\nNächster Zug: Hast du 2 X in einer \\nReihe?\",\n    \"Gehe in eine freie Ecke. \\n\\nNächster Zug: Hast du 2 X in einer \\nReihe?\"];\n\nif (runtimeScene.getTimeManager().isFirstFrame()) {\n  // Hide the first instance of PlayerHitBoxes.\n  for (let i = 0; i<=6; i++){\n        let txt = runtimeScene.createObject(\"IF_item_text\");\n        let obj = runtimeScene.createObject(\"IF_item\");\n        obj.setWidth(300);\n        obj.setHeight(100);\n        obj.setX(1600 - obj.getWidth() - 20);\n        obj.setY(i*110 + 20);\n        obj.setZOrder(2);\n        \n        txt.setWidth(280);\n        txt.setHeight(80);\n        txt.setX(obj.getX()+10);\n        txt.setY(obj.getY()+10);\n        txt.setZOrder(3);\n        txt.setString(texts[i]);\n        \n        obj.setVariableString(obj.getVariables().get(\"Name\"), \"Box\"+i);\n        txt.setVariableString(txt.getVariables().get(\"Source\"), \"Box\"+i);\n  }\n}\n\n\nobjects.forEach(item => {\n    if (item.getVariableString(item.getVariables().get(\"Source\")) != 0){\n        let obj = objects.find(ele => ele.getVariableString(ele.getVariables().get(\"Name\")) == item.getVariableString(item.getVariables().get(\"Source\")));\n        item.setX(obj.getX()+10);\n        item.setY(obj.getY()+10);\n    }\n});\n",
      "parameterObjects": "IF_group",
      "useStrict": true,
      "eventsSheetExpanded": true
    },
    {
      "disabled": false,
      "folded": false,
      "type": "BuiltinCommonInstructions::JsCode",
      "inlineCode": "if(runtimeScene.getGame().getInputManager().isMouseButtonPressed(0)){\n\n    let object = objects.find(item => item.getVariableBoolean(item.getVariables().get(\"dragged\"),true));\n\n    if(!object){\n        object = objects.find(item => {\n        let mouseX = runtimeScene.getGame().getInputManager().getMouseX();\n        let mouseY = runtimeScene.getGame().getInputManager().getMouseY();\n        if (item.getName() == \"IF_item\" && item.getX() <= mouseX && mouseX <= item.getX()+item.getWidth() && item.getY() <= mouseY && mouseY <= item.getY()+item.getHeight() ){\n            return true;\n        }\n        return false;\n    });\n    }\n    if(object){\n        object.setVariableBoolean(object.getVariables().get(\"dragged\"),true);\n        //get text + box to the front\n        object.setZOrder(50);\n        //find text\n        objects.forEach(item => {\n            if (item.getVariableString(item.getVariables().get(\"Source\")) != 0){\n                if(item.getVariableString(item.getVariables().get(\"Source\")) == object.getVariableString(object.getVariables().get(\"Name\"))){\n                    item.setZOrder(51);\n                }\n            }\n        });\n    }\n} else if (runtimeScene.getGame().getInputManager().isMouseButtonReleased(0)){\n    let object = objects.find(item => item.getVariableBoolean(item.getVariables().get(\"dragged\"),true));\n    if (object){\n        //get text + box to the back\n        object.setZOrder(2);\n        //find text\n        objects.forEach(item => {\n            if (item.getVariableString(item.getVariables().get(\"Source\")) != 0){\n                if(item.getVariableString(item.getVariables().get(\"Source\")) == object.getVariableString(object.getVariables().get(\"Name\"))){\n                    item.setZOrder(3);\n                }\n            }\n        });\n    }\n    objects.forEach(item => {\n        if (item.getName() == \"IF_item\"){\n           item.setVariableBoolean(item.getVariables().get(\"dragged\"),false) \n        }\n    });\n}\n",
      "parameterObjects": "IF_group",
      "useStrict": true,
      "eventsSheetExpanded": true
    }
  ],
